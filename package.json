{
  "name": "starlight-plugin-icons-monorepo",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.19",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "cd packages/starlight-plugin-icons && bun run build",
    "lint": "eslint . --fix",
    "dev": "cd packages/docs && bun run dev",
    "docs:build": "cd packages/docs && bun run build",
    "docs:check": "cd packages/docs && bun run check"
  }
}


