{
  "name": "starlight-plugin-icons-monorepo",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.2.21",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "dev": "cd packages/docs && bun run dev",
    "build": "cd packages/docs && bun run build",
    "preview": "cd packages/docs && bun run preview",
    "check": "cd packages/docs && bun run check",
    "update:deps": "taze latest -wr",
    "release": "bumpp packages/starlight-plugin-icons/package.json && npm publish --workspace starlight-plugin-icons"
  },
  "devDependencies": {
    "@iconify-json/devicon": "^1.2.43",
    "@iconify-json/logos": "^1.2.9",
    "bumpp": "^10.2.3",
    "taze": "^19.6.0"
  }
}
