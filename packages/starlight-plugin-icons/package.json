{
  "name": "starlight-plugin-icons",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "bun@1.2.19",
  "description": "Add icons to Astro Starlight: sidebar, codeblocks, file tree",
  "license": "MIT",
  "homepage": "https://github.com/Rettend/starlight-plugin-icons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rettend/starlight-plugin-icons.git"
  },
  "bugs": {
    "url": "https://github.com/Rettend/starlight-plugin-icons/issues"
  },
  "keywords": [
    "astro",
    "icons",
    "starlight"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./uno": {
      "types": "./dist/uno.d.ts",
      "import": "./dist/uno.js"
    },
    "./components/*": "./dist/components/*"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "node -e \"require('fs').copyFileSync('../../README.md', 'README.md')\"",
    "postpublish": "node -e \"require('fs').unlinkSync('README.md')\"",
    "build": "bun --bun tsup",
    "lint": "eslint . --fix",
    "update:deps": "taze latest -wr",
    "release": "bumpp && bun run build && npm publish"
  },
  "peerDependencies": {
    "@astrojs/starlight": ">=0.35.0",
    "astro": ">=4",
    "typescript": "^5",
    "unocss": ">=0.58.0",
    "zod": ">=3.22.0"
  },
  "dependencies": {
    "glob": "^11.0.3",
    "hast-util-select": "^6.0.4",
    "hast-util-to-string": "^3.0.1",
    "hastscript": "^9.0.1",
    "rehype": "^13.0.2",
    "unist-util-visit": "^5.0.0",
    "zod": ">=4.0.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "@types/bun": "latest",
    "@typescript/native-preview": "^7.0.0-dev.20250808.1",
    "@unocss/eslint-plugin": "^66.4.2",
    "astro-eslint-parser": "^1.2.2",
    "bumpp": "^10.2.2",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.32.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-format": "^1.0.1",
    "prettier-plugin-astro": "^0.14.1",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "taze": "^19.1.0",
    "tsup": "^8.5.0"
  }
}
